<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="software2project.LoginFormController">
   <children>
      <VBox layoutX="94.0" layoutY="68.0" prefHeight="157.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Label fx:id="usernameLabel" prefHeight="17.0" prefWidth="64.0" text="Username:" />
            <Label fx:id="passwordLabel" prefHeight="17.0" prefWidth="63.0" text="Password:" />
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
      <Label fx:id="welcomeLabel" layoutX="91.0" layoutY="17.0" prefHeight="36.0" prefWidth="107.0" text="Welcome">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="195.0" layoutY="76.0" promptText="Username" />
      <TextField fx:id="passwordTextField" layoutX="195.0" layoutY="113.0" promptText="Password" />
      <Button fx:id="signInButton" layoutX="265.0" layoutY="172.0" mnemonicParsing="false" onAction="#loggedIn" prefHeight="25.0" prefWidth="79.0" text="Sign In">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
